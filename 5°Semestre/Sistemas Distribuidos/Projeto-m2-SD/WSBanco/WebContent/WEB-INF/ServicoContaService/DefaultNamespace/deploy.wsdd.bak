<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ServicoContaService WSDL service -->

  <service name="ServicoConta" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="ServicoContaService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="ServicoConta"/>
      <parameter name="className" value="DefaultNamespace.ServicoContaSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ServicoConta"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sacarConta" qname="operNS:sacarConta" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:sacarContaReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idConta" xmlns:pns="http://DefaultNamespace" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:valorDeSaque" xmlns:pns="http://DefaultNamespace" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="depositarConta" qname="operNS:depositarConta" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:depositarContaReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idConta" xmlns:pns="http://DefaultNamespace" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:valorDeDeposito" xmlns:pns="http://DefaultNamespace" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="criarConta" qname="operNS:criarConta" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:criarContaReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nome" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:senha" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="consultarDadosConta" qname="operNS:consultarDadosConta" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:consultarDadosContaReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:idConta" xmlns:pns="http://DefaultNamespace" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sacarConta criarConta consultarDadosConta depositarConta"/>

  </service>
</deployment>
